{
    "name": "etapp-v2",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service build --watch",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "core-js": "^2.6.5",
        "font-awesome": "^4.7.0",
        "json-bigint": "^0.3.0",
        "uglifyjs-webpack-plugin": "^2.1.3",
        "vue": "^2.6.10",
        "vue-router": "^3.0.6",
        "vux": "^2.9.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.7.0",
        "@vue/cli-plugin-eslint": "^3.7.0",
        "@vue/cli-service": "^3.7.0",
        "babel-eslint": "^10.0.1",
        "better-scroll": "^1.15.2",
        "compression-webpack-plugin": "^2.0.0",
        "cos-js-sdk-v5": "^0.5.17",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "less": "~3.9.0",
        "less-loader": "^4.1.0",
        "vue-cli-plugin-vux": "^3.0.0",
        "vue-loader": "14.2.3",
        "vue-template-compiler": "^2.5.21",
        "vuex": "^3.1.0",
        "vux-loader": "^1.2.9",
        "weixin-js-sdk": "^1.4.0-test"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "generator-star-spacing": 0,
            "indent": 0,
            "no-debugger": 0,
            "no-console": 0,
            "no-cond-assign": 1,
            "no-extra-semi": 1,
            "no-extra-boolean-cast": 0,
            "no-useless-escape": 0,
            "no-unused-vars": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
